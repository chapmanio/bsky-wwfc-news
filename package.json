  "dependencies": {
    "@atproto/api": "^0.18.10",
    "@sentry/cloudflare": "^10.32.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260107.1",
    "oxlint": "^1.38.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "wrangler": "^4.57.0"
